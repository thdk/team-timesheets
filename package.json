{
  "name": "thdk-timesheets-app",
  "version": "0.7.0",
  "description": "A replacement web app for old school timesheets",
  "main": "src/js/app.ts",
  "scripts": {
    "build:refs": "tsc -p common",
    "build": "webpack --env=dev && gulp scss && gulp inject-css",
    "build:development": "webpack --env=dev && gulp scss && gulp inject-css",
    "build:production": "webpack --env=prod && gulp scss && gulp inject-css",
    "server": "firebase serve --only hosting",
    "deploy:development": "npm run build:development && firebase deploy",
    "deploy:production": "npm run build:production && firebase deploy",
    "tsc": "tsc -p common && tsc --noEmit",
    "test": "jest",
    "test:cloudbuild": "firebase emulators:exec --only firestore \"jest --coverage\"",
    "test:watch": "jest --watch",
    "emulator": "firebase emulators:start --only firestore",
    "emulator:exec": "firebase emulators:exec --only firestore",
    "test:emulator": "firebase emulators:exec --only firestore jest",
    "test:emulator:coverage": "firebase emulators:exec --only firestore \"jest --coverage\""
  },
